[run]
omit =
    */tests/*
    */test_*.py
    src/main.py
    src/utils/locations.py
    src/utils/repl_completer.py
    *.pyx

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
